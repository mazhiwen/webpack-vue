{
    "presets": ["react-app","@babel/preset-env"],
    // react-app 编译react以及jsx配置
    // preset-env es6以上编译为es5
    "plugins": [
        [
            //这是babel plugin import配置
            "import", 
            {
                "libraryName": "antd",
                "style": "css"   
                // or 'true' true会编译报错
            }
        ],
        // react class编译
        "transform-class-properties"
    ]
}